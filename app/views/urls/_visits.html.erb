<table class="table highlight">
  <thead>
    <tr>
      <th scope="col">IP Address</th>
      <th scope="col">Timestamp</th>
    </tr>
  </thead>
  <tbody>
    <% visits.each do |visit| %>
      <tr>
        <td><%= visit.ip_address %></td>
        <td><%= visit.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>